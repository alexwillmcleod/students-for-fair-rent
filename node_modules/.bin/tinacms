#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/alexw/Github/students-for-fair-rent/node_modules/.pnpm/@tinacms+cli@1.5.31_@babel+core@7.23.2_react-dom@18.2.0_react@18.2.0/node_modules/@tinacms/cli/bin/node_modules:/home/alexw/Github/students-for-fair-rent/node_modules/.pnpm/@tinacms+cli@1.5.31_@babel+core@7.23.2_react-dom@18.2.0_react@18.2.0/node_modules/@tinacms/cli/node_modules:/home/alexw/Github/students-for-fair-rent/node_modules/.pnpm/@tinacms+cli@1.5.31_@babel+core@7.23.2_react-dom@18.2.0_react@18.2.0/node_modules/@tinacms/node_modules:/home/alexw/Github/students-for-fair-rent/node_modules/.pnpm/@tinacms+cli@1.5.31_@babel+core@7.23.2_react-dom@18.2.0_react@18.2.0/node_modules:/home/alexw/Github/students-for-fair-rent/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/alexw/Github/students-for-fair-rent/node_modules/.pnpm/@tinacms+cli@1.5.31_@babel+core@7.23.2_react-dom@18.2.0_react@18.2.0/node_modules/@tinacms/cli/bin/node_modules:/home/alexw/Github/students-for-fair-rent/node_modules/.pnpm/@tinacms+cli@1.5.31_@babel+core@7.23.2_react-dom@18.2.0_react@18.2.0/node_modules/@tinacms/cli/node_modules:/home/alexw/Github/students-for-fair-rent/node_modules/.pnpm/@tinacms+cli@1.5.31_@babel+core@7.23.2_react-dom@18.2.0_react@18.2.0/node_modules/@tinacms/node_modules:/home/alexw/Github/students-for-fair-rent/node_modules/.pnpm/@tinacms+cli@1.5.31_@babel+core@7.23.2_react-dom@18.2.0_react@18.2.0/node_modules:/home/alexw/Github/students-for-fair-rent/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@tinacms/cli/bin/tinacms" "$@"
else
  exec node  "$basedir/../@tinacms/cli/bin/tinacms" "$@"
fi
